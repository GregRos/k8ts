{
  "name": "@k8ts/instruments",
  "version": "0.11.2",
  "description": "A collection of utilities and core components for k8ts.",
  "keywords": [
    "k8ts",
    "kubernetes",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/GregRos/k8ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:GregRos/k8ts",
    "directory": "packages/instruments"
  },
  "license": "MIT",
  "author": "GregRos <gregros@gregros.dev>",
  "contributors": [
    {
      "name": "GregRos",
      "email": "gregros@gregros.dev"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./timezone": {
      "require": "./dist/timezone.js",
      "import": "./dist/timezone.js",
      "default": "./dist/timezone.js",
      "types": "./dist/timezone.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "package.json",
    "LICENSE.md",
    "README.md",
    "tsconfig.json",
    "!**/tsconfig*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "watch": "tsc -b . --watch",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "dayjs": "^1.11.13",
    "doddle": "^7.3.1",
    "eventemitter3": "^5.0.1",
    "ip-address": "^10.0.1",
    "lodash": "^4.17.21",
    "parjs": "^1.3.9",
    "pathlib-js": "^1.5.0",
    "pathlib-ts": "^0.1.2",
    "simple-git": "^3.27.0",
    "stacktracey": "^2.1.8",
    "timezone-enum": "^1.0.4",
    "what-are-you": "^0.2.0"
  },
  "devDependencies": {
    "@k8ts/metadata": "workspace:^",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "22",
    "declare-it": "^2.1.0",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.8.2",
    "yamprint": "^0.8.4"
  },
  "peerDependencies": {
    "@k8ts/metadata": "workspace:^"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
