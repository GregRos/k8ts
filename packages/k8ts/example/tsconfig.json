{
  "extends": "../../../tsconfig.base.json",
  "references": [
    {
      "path": "../src/tsconfig.json"
    }
  ],
  "compilerOptions": {
    "rootDir": ".",
    "composite": false,
    "declarationMap": false,
    "outDir": "../dist_example",
    "noEmit": false,
    "tsBuildInfoFile": "../dist_example/test.tsbuildinfo",
    "types": ["jest", "node"],
    "paths": {
      "@lib": ["../src"],
      "@lib/*": ["../src/*"],
      "@imports": ["../src/_imports"],
      "@lib/kinds": ["../src/kinds/index"]
    }
  },
  "include": ["**/*.ts"]
}
