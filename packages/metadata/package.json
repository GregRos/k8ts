{
  "name": "@k8ts/metadata",
  "version": "0.8.0",
  "description": "K8ts tools for working with k8ts metadata.",
  "keywords": [
    "k8ts",
    "kubernetes",
    "typescript",
    "manifests",
    "metadata"
  ],
  "bugs": {
    "url": "https://github.com/GregRos/k8ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:GregRos/k8ts",
    "directory": "packages/metadata"
  },
  "license": "MIT",
  "author": "GregRos <gregros@gregros.dev>",
  "contributors": [
    {
      "name": "GregRos",
      "email": "gregros@gregros.dev"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./util": {
      "require": "./dist/util.js",
      "import": "./dist/util.js",
      "default": "./dist/util.js",
      "types": "./dist/util.d.ts"
    }
  },
  "main": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./util": [
        "./dist/util/index.d.ts"
      ]
    }
  },
  "typings": "dist/index",
  "files": [
    "src",
    "dist",
    "package.json",
    "LICENSE.md",
    "README.md",
    "!**/tsconfig*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "watch": "tsc -b . --watch",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "doddle": "^7.2.0",
    "parjs": "^1.3.9",
    "what-are-you": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "20.9.1",
    "declare-it": "^2.1.0",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.8.2",
    "yamprint": "^0.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
