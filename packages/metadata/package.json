{
  "name": "@k8ts/metadata",
  "version": "0.0.1",
  "description": "Abstractions for working with Kubernetes metadata",
  "keywords": [
    "kubernetes",
    "typescript",
    "manifests",
    "build",
    "helm",
    "cdk8s"
  ],
  "bugs": {
    "url": "https://github.com/GregRos/k8ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:GregRos/k8ts",
    "directory": "packages/metadata"
  },
  "license": "MIT",
  "author": "GregRos <gregros@gregros.dev>",
  "contributors": [
    {
      "name": "GregRos",
      "email": "gregros@gregros.dev"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "typings": "dist/index",
  "files": [
    "src",
    "dist",
    "package.json",
    "LICENSE.md",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "watch": "tsc -b . --watch",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "resolutions": {
    "doddle": "portal:../doddle"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "declare-it": "^2.1.0",
    "doddle": "^5.0.0-beta.13",
    "emittery": "0.13.1",
    "immutable": "^5.0.3",
    "lodash": "^4.17.21",
    "ora": "5.4.1",
    "parjs": "^1.3.9",
    "shx": "^0.3.4",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "20.9.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2",
    "yamprint": "^0.8.4"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=16.0.0"
  }
}
