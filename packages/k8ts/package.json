{
  "name": "k8ts",
  "version": "0.1.0",
  "description": "Powerful framwork for building Kubernetes manifests in TypeScript.",
  "keywords": [
    "kubernetes",
    "typescript",
    "manifests",
    "build",
    "helm",
    "cdk8s"
  ],
  "bugs": {
    "url": "https://github.com/GregRos/k8ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:GregRos/k8ts",
    "directory": "packages/metadata"
  },
  "license": "MIT",
  "author": "GregRos <gregros@gregros.dev>",
  "contributors": [
    {
      "name": "GregRos",
      "email": "gregros@gregros.dev"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api-versions": {
      "require": "./dist/api-versions.js",
      "import": "./dist/api-versions.js",
      "default": "./dist/api-versions.js",
      "types": "./dist/api-versions.d.ts"
    }
  },
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "src",
    "dist",
    "package.json",
    "LICENSE.md",
    "README.md",
    "proverbs.txt"
  ],
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "watch": "tsc -b . --watch",
    "clean": "shx rm -rf dist dist_test",
    "test": "tsx --tsconfig example/tsconfig.json example/run.ts",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@k8ts/instruments": "workspace:^",
    "@k8ts/metadata": "workspace:^",
    "chalk": "4.1.2",
    "constructs": "^10.4.2",
    "dayjs": "^1.11.13",
    "doddle": "^5.0.0-beta.13",
    "emittery": "0.13.1",
    "immutable": "^5.0.3",
    "lodash": "^4.17.21",
    "ora": "5.4.1",
    "parjs": "^1.3.9",
    "simple-git": "^3.27.0",
    "stacktracey": "^2.1.8"
  },
  "devDependencies": {
    "@datreeio/datree-cdk8s": "^1.3.5",
    "@swc/core": "^1.11.11",
    "@swc/jest": "^0.2.37",
    "@types/chalk": "^2.2.4",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "20.9.1",
    "cdk8s": "^2.69.48",
    "cdk8s-cli": "^2.200.7",
    "declare-it": "^2.1.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "yamprint": "^0.8.4"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=16.0.0"
  }
}
